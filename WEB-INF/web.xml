<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>InsertReport</servlet-name>
        <servlet-class>bomb.InsertReport</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertReport</servlet-name>
        <url-pattern>/InsertReport</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetAllReport</servlet-name>
        <servlet-class>bomb.GetAllReport</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetAllReport</servlet-name>
        <url-pattern>/GetAllReport</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>InsertProductServlet </servlet-name>
        <servlet-class>bomb.InsertProductServlet </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertProductServlet </servlet-name>
        <url-pattern>/InsertProductServlet </url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UpdateReport</servlet-name>
        <servlet-class>bomb.UpdateReport </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateReport</servlet-name>
        <url-pattern>/UpdateReport</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UpdateProductsServlet</servlet-name>
        <servlet-class>bomb.UpdateProductsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateProductsServlet</servlet-name>
        <url-pattern>/UpdateProductsServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteReport</servlet-name>
        <servlet-class>bomb.DeleteReport </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteReport</servlet-name>
        <url-pattern>/DeleteReport</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CustomerInsertServlet</servlet-name>
        <servlet-class>bomb.CustomerInsertServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CustomerInsertServlet</servlet-name>
        <url-pattern>/CustomerInsertServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetALLShippingDetailsServlet</servlet-name>
        <servlet-class>bomb.GetALLShippingDetailsServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GetALLShippingDetailsServlet</servlet-name>
        <url-pattern>/getAllShippingDetails</url-pattern>
    </servlet-mapping>

    <!--User System-->
    <servlet>
        <servlet-name>UserRegisterServlet</servlet-name>
        <servlet-class>user.UserRegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserRegisterServlet</servlet-name>
        <url-pattern>/UserRegisterServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserLoginServlet</servlet-name>
        <servlet-class>user.UserLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserLoginServlet</servlet-name>
        <url-pattern>/UserLoginServlet</url-pattern>
    </servlet-mapping>



</web-app>