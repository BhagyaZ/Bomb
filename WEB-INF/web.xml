<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    
    <servlet>
        <servlet-name>InsertReport</servlet-name>
        <servlet-class>bomb.InsertReport</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertReport</servlet-name>
        <url-pattern>/InsertReport</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetAllReport</servlet-name>
        <servlet-class>bomb.GetAllReport</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetAllReport</servlet-name>
        <url-pattern>/GetAllReport</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>InsertProductServlet </servlet-name>
        <servlet-class>bomb.InsertProductServlet </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertProductServlet </servlet-name>
        <url-pattern>/InsertProductServlet </url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UpdateReport</servlet-name>
        <servlet-class>bomb.UpdateReport </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateReport</servlet-name>
        <url-pattern>/UpdateReport</url-pattern>
    </servlet-mapping>

</web-app>